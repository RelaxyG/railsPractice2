<h1>
  All Resaturants
</h1>
<div class="flex">
  <% @restaurants.each do |restaurant| %>
    <div class="inner">
      <h3><%=restaurant.name%></h3>
      <h4><%=restaurant.address%></h4>
      <h4><%=restaurant.contact%></h4>
      <h4><%= "⭐" * restaurant.rating%></h4>
    </div>
  <% end %>
</div>
